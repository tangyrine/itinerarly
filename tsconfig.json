{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["es2018", "dom", "esnext.asynciterable"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*",
        "./src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.geojson",
    ".next/types/**/*.ts",
    "app/auth/callback/route.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
